<div class="title-container">
    <div>  
      <h2>Events in your city</h2>
    </div>
</div>

  <% results.forEach((event) => { %>
    <% if (event.active) { %>
    <div class="event-container">
      <div class="event-description">  
        <h3><%= event.title %></h3>
        <p><%= event.description %></p>
      </div>
      <div class="event-attend-details">   
        <p><%= event.attendees.length %> people will assist</p>
        <form action="/events/<%= event._id%>" method="GET">
          <button class="btn-main">View details</button>
        </form>
      </div>
    </div>
    <% } %>
  <% }) %>
